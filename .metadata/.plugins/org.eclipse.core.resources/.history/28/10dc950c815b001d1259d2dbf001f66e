package Interfaz;

import javax.swing.JButton;
import javax.swing.JFrame;

public class VentanaEstadisticas extends JFrame implements ActionListener {
	private JButton btnEstEquipo;
	private JButton btnEstJornada;
	private JButton btnEstTemporada;
	private InterfazPrincipal padre;
	public VentanaEstadisticas(InterfazPrincipal papa) {
		padre = papa;
		
		btnEstEquipo = new JButton("Resultados del equipo");
		btnEstEquipo.setActionCommand(COMPRAR);
		btnEstEquipo.addActionListener(this);
		
		btnEstJornada = new JButton("Resultados del equipo");
		btnEstJornada.setActionCommand(COMPRAR);
		btnEstJornada.addActionListener(this);
		
		btnEstTemporada = new JButton("Resultados del equipo");
		btnEstTemporada.setActionCommand(COMPRAR);
		btnEstTemporada.addActionListener(this);
		
	}
}
